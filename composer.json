{
  "name": "brandonlinu/cvs-updater-bundler",
  "type": "symfony-bundle",
  "description": "A Symfony Bundler to update the source code with a CVS remote server",
  "keywords": ["Symfony", "Updater", "CVS", "Github"],
  "homepage": "https://github.com/BrandonlinU/SymfonyCvsUpdaterBundler",
  "license": "MIT",
  "version": "0.1.0",
  "authors": [
    {
      "name": "Brandon Antonio",
      "email": "brandolin_a_@outlook.com",
      "homepage": "https://github.com/BrandonlinU"
    }
  ],
  "require": {
    "php": "^7.3 || ^8.0",
    "ext-json": "*",
    "symfony/console": "^4.4 || ^5.2",
    "symfony/dependency-injection": "^4.4 || ^5.2",
    "symfony/flex": "^1.3.1",
    "symfony/framework-bundle": "^4.4 || ^5.2",
    "symfony/process": "^4.4 || ^5.2"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "symfony/browser-kit": "^4.4 || ^5.2",
    "symfony/phpunit-bridge": "^4.4 || ^5.2",
    "symfony/yaml": "^4.4 || ^5.2"
  },
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "BrandonlinU\\CvsUpdaterBundler\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BrandonlinU\\CvsUpdaterBundler\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "@php bin/phpunit"
  }
}
